<?xml version="1.0" encoding="utf-8"?>
<extension
    type="module"
    version="2.5"
    client="site"
    method="upgrade">
    <name>Moo Carousel</name>
    <author>Dan Mooney</author>
    <creationDate>February 2013</creationDate>
    <copyright>Copyright (C) 2013 DanRonMoon</copyright>
    <authorEmail>dan.r.mooney@gmail.com</authorEmail>
    <authorUrl>www.danronmoon.com</authorUrl>
    <description>Lists out images for carousel</description>
    <files>
    </files>
    <config>
        <fields name="params">
            <fieldset name="basic">
                <field name="speed" default="6" type="text" label="Delay (seconds)" description="The number of seconds to pause carousel before going to next slide" />
            </fieldset>
        </fields>
    </config>
</extension>
